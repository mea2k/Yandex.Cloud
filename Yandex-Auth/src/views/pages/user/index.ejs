<%- include('../../layouts/header', {title, user}) %>


	<h1>
		<%= title %>
	</h1>

	<% if(user) { %>

		<h2>Профиль пользователя <%= user.name %>
		</h2>
		<ol>
			<% for(var key in Object.keys(user)) { %>
				<ol><b>
						<%= key %>
					</b>
					<%= user[key] %>
				</ol>
			<% } %>
		</ol>
	<% } else { %>
		<%- include('logreg_form') %>
	<% } %>



	<%- include('../../layouts/footer') %>
